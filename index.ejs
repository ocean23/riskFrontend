	<body>
	<script type="text/javascript">
		const fullHost = location.protocol+'//'+location.hostname+(location.port ? ':'+location.port: '');
		const context = 'web';
		var G = G || {
			baseHost: '<%= htmlWebpackPlugin.options.baseHost%>',
			realHost: fullHost,
			serverHost: fullHost+'/'+context
		};
		G.id = function(id) {
			return document.getElementById(id);
		};
		G.showLoading = function() {
			G.id('g-loading').style.display = 'block';
		};
		G.hideLoading = function() {
			G.id('g-loading').style.display = 'none';
		};
		var url_obj = parser(window.location.href);
		var code = url_obj.search.code;
		if (code === null || code === undefined) {
			window.location.href = 'redirecthome.html';
		} else {
			G.code = code;
		}
	</script>
		<div id="g-loading" class="circleG_wrapper">
			<div class="circleG circleG_1"></div>
			<div class="circleG circleG_2"></div>
			<div class="circleG circleG_3"></div>
		</div>
		<div id="root">
		</div>
	</body>
